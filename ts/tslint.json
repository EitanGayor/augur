{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "no-unused-expression": true,
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "interface-name": [true, "never-prefix"],
      "max-line-length": [false, 100],
      "indent": [true, "spaces", 2]
    },
    "rulesDirectory": []
  }